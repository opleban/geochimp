<div class="content_container">
  <h1>GeoMonkey</h1>

  <div id="map" class=".span_6_of_9"></div>

  <div id="surveys_container" class="float_left">
    <h2>Surveys</h2>
    <% if @surveys %>
    <ul>
      <% @surveys.each do |survey| %>
      <li id="s<%= survey.id %>">
        <%= survey.title %>
      </li>
      <% end %>
    </ul>
    <% end %>
  </div>
</div>
